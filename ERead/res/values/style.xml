<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <style name="ListItem">
    </style>
	<style name="ListView">
        <item name="android:background">@color/color_4</item>
        <item name="android:dividerHeight">1px</item>
        <item name="android:divider">#ffA0A0A0</item>
        <item name="android:paddingLeft">5dp</item>
    </style>
    <style name="ListItemH1">
        <item name="android:textColor">#ff0f0f0f</item>
        <item name="android:textSize">18sp</item>
    </style>

    <style name="ListItemH2">
        <item name="android:textColor">#ff0f0f0f</item>
        <item name="android:textSize">16sp</item>
    </style>

    <style name="ListItemNormal1">
        <item name="android:textColor">#ff0f0f0f</item>
        <item name="android:textSize">12sp</item>
    </style>
	<style name="TextNormal1">
        <item name="android:textColor">#ff0f0f0f</item>
        <item name="android:textSize">14sp</item>
    </style>
    <style name="CustomWindowTitleText" parent="android:TextAppearance.WindowTitle">
        <item name="android:textSize">20sp</item>
        <item name="android:textColor">#fff0f0f0</item>
    </style>

    <style name="HeaderBackground">
        <item name="android:background">@color/color_1</item>
         <item name="android:icon">@drawable/icon</item>
    </style>
   <style name="ButtonBar">
        <item name="android:background">@color/color_2</item>
       
    </style>
    <style name="SeekBar">
        <item name="android:progressDrawable">@drawable/seekbar</item>
        <item name="android:thumb">@drawable/seekbarpin</item>
        <item name="android:paddingLeft">10dp</item>
        <item name="android:paddingRight">10dp</item>
        <item name="android:paddingTop">5dp</item>
        <item name="android:thumbOffset">5dp</item>
    </style>
    <style name="Body">
        <item name="android:background">@color/color_4</item>
       
    </style>
    <style name="Button">
        <item name="android:background">@drawable/button</item>
         <item name="android:padding" >10dp</item>
         <item name="android:layout_margin">3dp</item>
    	<item name="android:textSize" >18sp</item>
    	 <item name="android:textColor">#fff0f0f0</item>
    </style>
    <style name="Header" parent="android:Theme">
        <item name="android:textAppearance">@style/CustomWindowTitleText</item>
        <item name="android:paddingLeft">5dp</item>
        <item name="android:shadowDx">0</item>
        <item name="android:shadowDy">0</item>
        <item name="android:shadowRadius">5</item>
        <item name="android:shadowColor">#1155CC</item>
    </style>

    <style name="Button1">
        <item name="android:background">#ff193563</item>
    </style>

    <style name="MyTheme" parent="android:Theme">
        <item name="android:windowTitleSize">48dp</item>
        <item name="android:windowTitleStyle">@style/Header</item>
        <item name="android:windowTitleBackgroundStyle">@style/HeaderBackground</item>
        <item name="android:actionBarStyle">@style/HeaderBackground</item>
    </style>

</resources>